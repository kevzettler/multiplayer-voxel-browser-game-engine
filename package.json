{
  "name": "multiplayer-voxel-browser-engine",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -l -p client:dev server:dev server:signalhub",
    "client:dev": "npx webpack-dev-server --mode development --config ./webpack/web.dev.js",
    "client:build": "npx webpack --progress --config ./webpack/web.prod.js",
    "server:dev": "sleep 5 && npm-run-all -l -p server:watch server:start",
    "server:watch": "npx webpack --progress --config ./webpack/server.dev.js",
    "server:start": "sleep 12 && nodemon --inspect build/server.js",
    "server:build": "npx webpack --progress --config ./webpack/server.prod.js",
    "server:signal": "npx signalhub listen -p 8080",
    "local-prod-client": "NODE_ENV=production ASSET_HOST=localhost ASSET_PORT=5000 SIGNALHUB_HOST=0.0.0.0 SOCKETIO_HOST=0.0.0.0 TURNSERVER_HOST=0.0.0.0 npm run build-client && cp -R ./public/assets ./build && npx serve ./build",
    "local-prod-server": "NODE_ENV=production ASSET_HOST=localhost ASSET_PORT=5000 SIGNALHUB_HOST=0.0.0.0 SOCKETIO_HOST=0.0.0.0 TURNSERVER_HOST=0.0.0.0 npm run build-server && node build/server.js",
    "meshes:qb": "find ./assets -name \"*.qb\" | while IFS= read -r file_path; do node ./scripts/QBToAOVerts.js \"$file_path\"; done",
    "meshes:vox": "find ./assets -name \"*.qb\" | while IFS= read -r file_path; do node ./scripts/QBToAOVerts.js \"$file_path\"; done",
    "meshes:clean": "git clean -f -e '!*.aoverts'",
    "meshes:build": "npm run meshes:qb && npm run meshes:vox"
  },
